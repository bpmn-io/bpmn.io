<script>
  function getCookie(name) {
    let matches = document.cookie.match(new RegExp(
      "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
    ));

    console.log('get cookie', matches ? decodeURIComponent(matches[1]) : undefined);

    return matches ? decodeURIComponent(matches[1]) : undefined;
  }

  function setCookie() {
    console.log('set cookie')
    document.cookie = "hideCCBanner=true; expires=Fri, 29 Sep 2023 00:00:00 GMT";
  }

  function hideBanner() {
    console.log('hide banner')
    document.querySelector('.cc-banner').style.display = 'none';
  }

  function dismissBanner() {
    console.log('dismiss banner')
    hideBanner();

    setCookie();
  }

  document.addEventListener("DOMContentLoaded", () => {
    if (getCookie('hideCCBanner')) {
      hideBanner();
    }
  });
</script>

